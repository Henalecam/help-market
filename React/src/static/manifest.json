{
  "manifest_version": 3,
  "name": "HelMart",
  "version": "1.0",
  "description": "HelMart",
  "icons": {
    "16": "./icon.png"
  },
  "action": {
    "default_icon": "./icon.png",
    "default_title": "HelMart",
    "default_popup": "./popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "service_worker": "./background.js"
  },
  "options_page": "./options.html"
}